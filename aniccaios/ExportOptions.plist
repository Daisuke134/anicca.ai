<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<!-- Distribution method: app-store, ad-hoc, enterprise, development -->
	<key>method</key>
	<string>app-store</string>

	<!-- Team ID -->
	<key>teamID</key>
	<string>S5U8UH3JLJ</string>

	<!-- Bitcode is deprecated and no longer accepted -->
	<key>uploadBitcode</key>
	<false/>

	<!-- Upload symbols for crash reports -->
	<key>uploadSymbols</key>
	<true/>

	<!-- Bitcode compilation (deprecated) -->
	<key>compileBitcode</key>
	<false/>

	<!-- Signing style: automatic or manual -->
	<!-- Using automatic since we changed CODE_SIGN_STYLE to Automatic -->
	<key>signingStyle</key>
	<string>automatic</string>

	<!-- Signing certificate: leave empty for automatic signing -->
	<!-- Xcode will select the appropriate certificate based on the team -->
	<key>signingCertificate</key>
	<string>Apple Distribution</string>

	<!-- Provisioning profiles (optional for automatic signing) -->
	<!-- Uncomment if you need to specify a specific provisioning profile
	<key>provisioningProfiles</key>
	<dict>
		<key>ai.anicca.app.ios</key>
		<string>YOUR_PROVISIONING_PROFILE_NAME</string>
	</dict>
	-->

	<!-- Strip Swift symbols to reduce IPA size -->
	<key>stripSwiftSymbols</key>
	<true/>

	<!-- Generate app size report -->
	<key>generateAppStoreInformation</key>
	<true/>

	<!-- Destination: export, upload -->
	<key>destination</key>
	<string>export</string>
</dict>
</plist>
